  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-Q1ULrzhTZMBs7rZTcdNYMw" name="survey_protocol,_e2ZJ1H3CEeqCLqklk7niQg" guid="-Q1ULrzhTZMBs7rZTcdNYMw">
