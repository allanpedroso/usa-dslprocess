  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-RCJeFfB7K4WMphBYVf5Dkg" name="e4b_-_review_the_evaluation_study_protocol,_yprNwUZYEemUh_IZAUpcPQ" guid="-RCJeFfB7K4WMphBYVf5Dkg">
