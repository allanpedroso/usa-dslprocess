  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-ztwdo8J_8LKrEWW145_AtA" name="survey_protocol,_e55JwH3CEeqCLqklk7niQg" guid="-ztwdo8J_8LKrEWW145_AtA">
    <refinedDescription>&lt;p dir=&quot;ltr&quot;>&#xD;
    &lt;strong>&lt;span>a ) Background&lt;/span>&lt;/strong>&#xD;
&lt;/p>&#xD;
&lt;ol style=&quot;PADDING-LEFT: 30px&quot;>&#xD;
    &lt;li>&#xD;
        &lt;div>&#xD;
