        guid=&quot;_dumskUZZEemUh_IZAUpcPQ&quot;>(Activity A1)&lt;/a>&#xD;
    &lt;/p>&#xD;
    &lt;p dir=&quot;ltr&quot;>&#xD;
        &lt;span>&lt;span style=&quot;COLOR: #000000&quot;>Analyze the performed Tasks &lt;a class=&quot;elementLinkWithUserText&quot;&#xD;
