  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-Z9k1x-qTmTM7JdI0iAbTJA" name="survey_protocol,_e3MbEX3CEeqCLqklk7niQg" guid="-Z9k1x-qTmTM7JdI0iAbTJA">
